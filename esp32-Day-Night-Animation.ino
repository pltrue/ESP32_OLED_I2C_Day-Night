int frameDay = 0;
int frameNight = 0;

void setup() {
  Serial.begin(115200);

  // เชื่อมต่อ WiFi
  WiFi.begin(ssid, password);
  Serial.print("Connecting to WiFi");
  while (WiFi.status() != WL_CONNECTED) {
    delay(500);
    Serial.print(".");
  }
  Serial.println(" Connected!");

  // ตั้ง Timezone เป็น UTC+7 (ประเทศไทย)
  configTime(7 * 3600, 0, "pool.ntp.org", "time.nist.gov");

  // เริ่ม OLED
  display.begin(SSD1306_SWITCHCAPVCC, SCREEN_ADDRESS);
  display.clearDisplay();
  display.display();
}

void loop() {
  struct tm timeinfo;
  if (!getLocalTime(&timeinfo)) {
    Serial.println("Failed to obtain time");
    delay(1000);
    return;
  }

  int currentHour = timeinfo.tm_hour; // เอาเวลาจริงมาเทียบ

  display.clearDisplay();

  // 1. วาด Animation (แค่ 80% ของจอ)
  if (currentHour >= 6 && currentHour < 18) {
    // กลางวัน
    display.drawBitmap(
      (SCREEN_WIDTH - FRAME_WIDTH) / 2, 0,
      framesDay[frameDay],
      FRAME_WIDTH, FRAME_HEIGHT,
      1
    );
    frameDay = (frameDay + 1) % FRAME_COUNT_DAY;
  } else {
    // กลางคืน
    display.drawBitmap(
      (SCREEN_WIDTH - FRAME_WIDTH) / 2, 0,
      framesNight[frameNight],
      FRAME_WIDTH, FRAME_HEIGHT,
      1
    );
    frameNight = (frameNight + 1) % FRAME_COUNT_NIGHT;
  }

  // 2. วาดเส้นแนวนอน (แบ่ง 80%/20%)
  display.drawLine(0, 50, SCREEN_WIDTH, 50, SSD1306_WHITE);

  // 3. แสดงวัน/เดือน/ปี
  display.setTextSize(1);
  display.setTextColor(SSD1306_WHITE);

  display.setCursor(0, 52);
  char dateStr[20];
  strftime(dateStr, sizeof(dateStr), "%d/%m/%Y", &timeinfo);
  display.print(dateStr);

  // 4. แสดงเวลา ชั่วโมง:นาที
  display.setCursor(97, 52); // ชิดขวานิดนึง
  char timeStr[10];
  strftime(timeStr, sizeof(timeStr), "%H:%M", &timeinfo);
  display.print(timeStr);

  display.display();
  delay(FRAME_DELAY);
}
